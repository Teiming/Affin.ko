echo ">원문을 입력합니다. 대소문자와 공백을 구별합니다."
read ENG
echo ">번역할 내용을 입력합니다. 마찬가지로 공백을 구별합니다."
read KOR
cd ~/Affinity-in-Korean/
ENG1=$(echo $ENG | sed -e "s/ \/ / \\\\\/ /" | sed -e "s/\[/\\\[/")
KOR1=$(echo $KOR | sed -e "s/ \/ / \\\\\/ /" | sed -e "s/\[/\\\[/")
exp=$(echo "s/g>$ENG1</g>$KOR1</")
echo $exp >> DictionaryDesigner
echo $exp >> DictionaryPhoto
/bin/cat DictionaryDesigner | /usr/bin/sort | /usr/bin/uniq > DictionaryDesigner.temp && /bin/mv DictionaryDesigner.temp DictionaryDesigner
/bin/cat DictionaryPhoto | /usr/bin/sort | /usr/bin/uniq > DictionaryPhoto.temp && /bin/mv DictionaryPhoto.temp DictionaryPhoto
diff DictionaryDesigner DictionaryPhoto | grep "s" > DictionaryDiff
/bin/cat DictionaryDesigner | grep "&" >> DictionaryDiff
/bin/cat DictionaryPhoto | grep "&" >> DictionaryDiff
./affdic
